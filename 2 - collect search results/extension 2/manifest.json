{
  "name" : "Extension 2",
  "version" : "1.0",
  "description": "WebMedia 2019 - Short Course 2 - Extension 2.",
  "permissions" : ["tabs"],
  "browser_action" : {
    "default_popup": "popup.html",
    "default_icon" : "icon2.png"
  },
	"content_scripts": [
	    {
	      "matches": ["https://www.google.com/*"],
      	"js": ["content.js"]
	    }
	  ],
  "background" : {
    "scripts" : ["background.js"],
    "persistent" : false
  },
	"manifest_version" : 2
}
